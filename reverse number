import java.util.*;
public class numberreverse 
{
    public static void main(String[] args)
     {
        int rem=0;
        Scanner sc = new Scanner(System.in);
        System.out.print("Enter the number: ");
        int n = sc.nextInt();
        System.out.print("reversed number:");
        while(n!=0)
        {
            rem=n%10;
            System.out.print(rem);
            n=n/10;
        }
     }
}
