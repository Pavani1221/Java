import java.util.*;
public class floydsquare {
    public static void main(String args[])
    {
    System.out.print("enter n:");
    Scanner sc=new Scanner(System.in);
    int n=sc.nextInt();
    int num=1;
    System.out.println("square triangle");
    for(int i=1;i<=n;i++)
    {
        for(int j=1;j<=i;j++) 
        {
            System.out.print(num*num+"  ");
            num++;
        }
        System.out.println();
    }
    
}}
