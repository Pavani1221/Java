import java.util.*;
public class remvow 
{
    public static void main(String args[])
    {
        System.out.print("enter the string:");
        Scanner sc=new Scanner(System.in);
        String str=sc.nextLine();
        String str1="";
        str1=str.replaceAll("[aeiouAEIOU]","");
        System.out.println("the replaced string:"+str1);
    
    }
}
